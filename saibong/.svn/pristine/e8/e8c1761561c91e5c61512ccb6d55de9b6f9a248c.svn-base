		  	#{dqlsh,jdbcType=INTEGER},
		  	#{gvalue,jdbcType=VARCHAR},
		  	#{tvalue,jdbcType=VARCHAR}, 
		  	#{lvalue,jdbcType=VARCHAR},
		  	#{fssj,jdbcType=TIMESTAMP} 
		)
	</insert>
	
</mapper>