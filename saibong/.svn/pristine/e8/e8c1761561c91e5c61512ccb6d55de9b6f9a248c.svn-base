		insert into 
			SAIBONG_XX_TWO  (CFKEY, DPRTCODE, DJH, DQFH, DQLSH, G_VALUE, T_VALUE, L_VALUE, FSSJ)
		values
		(
			#{cfkey,jdbcType=VARCHAR},
		  	#{dprtcode,jdbcType=VARCHAR},
		  	#{djh,jdbcType=VARCHAR},
