	<subDataset name="付款明细" uuid="8cc4b150-1ab1-4826-8a44-dfdabaee4c88">
		<parameter name="sql" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[
select b1.id,
       b1.hth,
       b1.jjcd,
       b1.dprtcode,
       b1.htqdrq,
       b1.htlx,
       b1.gysbm,
       b1.gysmc,
       b2.fkrq,
       case fkfs
         when 0 then
          b2.fkfssm
         when 1 then
          '支票'
         when 2 then
          '现金'
         when 3 then
          '汇款'
         else
          '-'
       end fkfs,
       b2.fkje,
       b2.bz,
       t.dprtname
  from b_h_00402 b2
  join b_h_004 b1
    on b2.mainid = b1.id
   and b1.zt in (2, 10)
   and b2.zt = 1
   left join t_department t on t.id=b1.dprtcode
$P!{sql}
order by htqdrq desc, htlx asc, hth asc, fkrq asc
]]>
		</queryString>
