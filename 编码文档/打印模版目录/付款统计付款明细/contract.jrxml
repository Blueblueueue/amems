	<subDataset name="付款统计" uuid="43341397-81d1-46c1-9392-5ea785b35390">
		<parameter name="sql" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select b1.id,
            b1.hth,
              b1.jjcd,
                b1.dprtcode,
                 b1.htqdrq,
                 b1.htlx,
                 b1.gysbm,
                 b1.gysmc,
                 trunc((nvl(b1.fjfy, 0) + nvl(b3.fy,0)), 2) yf,
                 trunc((nvl(b1.fjfy, 0) + nvl(b3.fy,0) - nvl(b2.fkje,0)), 2) wf,
                 case
                 when (nvl(b1.fjfy, 0) + nvl(b3.fy,0)) = 0 then
                    0
                 else
                  trunc(nvl(b2.fkje,0) / (nvl(b1.fjfy, 0) + nvl(b3.fy,0)) * 100, 2)
                   end yfbl,
                   b2.fkcs,
                 case
                   when nvl(b3.htsl,0) = 0 then
                      0
                   else
                      trunc(nvl(b3.dhsl,0) / b3.htsl * 100, 2)
                 end dhbl,t.dprtname
            from b_h_004 b1
            join (select mainid,
                       count(1) fkcs,
                       sum(fkje) fkje
                  from b_h_00402
                 where zt = 1
                 group by mainid) b2
            on b2.mainid = b1.id
           and b1.zt in (2,10)
          join (select mainid,
                       sum((nvl(ht_clf, 0) + nvl(ht_gsf, 0) + nvl(ht_qtf, 0)) *
                           nvl(ht_sl, 0)) fy,
                       sum(ht_sl) htsl,
                       sum(dhsl) dhsl
                  from b_h_00401
                 where zt = 1
                 group by mainid) b3
            on b1.id = b3.mainid
            left join t_department t on b1.dprtcode=t.id
	$P!{sql}
        order by htqdrq desc, htlx asc, hth asc]]>
		</queryString>
		<field name="ID" class="java.lang.String"/>
		<field name="HTH" class="java.lang.String"/>
		<field name="JJCD" class="java.math.BigDecimal"/>
		<field name="DPRTCODE" class="java.lang.String"/>
		<field name="HTQDRQ" class="java.sql.Timestamp"/>
		<field name="HTLX" class="java.math.BigDecimal"/>
		<field name="GYSBM" class="java.lang.String"/>
		<field name="GYSMC" class="java.lang.String"/>
		<field name="YF" class="java.math.BigDecimal"/>
		<field name="WF" class="java.math.BigDecimal"/>
		<field name="YFBL" class="java.math.BigDecimal"/>
		<field name="FKCS" class="java.math.BigDecimal"/>
		<field name="DHBL" class="java.math.BigDecimal"/>
		<field name="DPRTNAME" class="java.lang.String"/>
		<variable name="index" class="java.lang.Integer" calculation="Count">
			<variableExpression><![CDATA[$V{index}]]></variableExpression>
			<initialValueExpression><![CDATA[1]]></initialValueExpression>
