		<queryString>
			<![CDATA[select
    B.ID, B.MAINID, B.GYSID, NVL(D.GYSBM,B.GYSBM) GYSBM, NVL(D.GYSMC,B.GYSMC) GYSMC, B.BJ_CLF, B.BJ_GSF, B.BJ_QTF, B.YJDHRQ, B.BZ, B.ZT, B.WHDWID,
    B.WHRID, B.WHSJ,U.USERNAME AS XJRUSERNAME,U.REALNAME AS XJRREALNAME,D.HZZK,D.GYSDJ
    from B_H_0020101 B
    left join T_USER U
    on B.WHRID = U.ID
    left join D_010 D
    on B.GYSID = D.ID
    where B.ZT = 1 AND B.MAINID = $P{mainid}]]>
		</queryString>
		<field name="ID" class="java.lang.String"/>
		<field name="MAINID" class="java.lang.String"/>
		<field name="GYSID" class="java.lang.String"/>
		<field name="GYSBM" class="java.lang.String"/>
		<field name="GYSMC" class="java.lang.String"/>
		<field name="BJ_CLF" class="java.math.BigDecimal"/>
		<field name="BJ_GSF" class="java.math.BigDecimal"/>
